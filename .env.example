# Environment variables for Modal Armor

# OpenAI API Key (if using OpenAI embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# API Security
MODAL_ARMOR_API_KEY=your_secret_api_key_here

# Server Configuration
MODAL_ARMOR_HOST=0.0.0.0
MODAL_ARMOR_PORT=5000
MODAL_ARMOR_DEBUG=false

# Database
MODAL_ARMOR_DB_PATH=data/vectordb

# Logging
LOG_LEVEL=INFO
LOG_PATH=logs/modal_armor.log

# Performance
MAX_WORKERS=4
REQUEST_TIMEOUT=30

# Monitoring (optional)
PROMETHEUS_ENABLED=false
PROMETHEUS_PORT=9090
